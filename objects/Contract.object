    <validationRules>
        <fullName>Contract_Dates_Validation_Rule</fullName>
        <active>true</active>
        <description>Start Contract date should be before End Contract date</description>
        <errorConditionFormula>AND( NOT(ISBLANK( StartDate  )) ,NOT(ISBLANK( EndDate )), StartDate &gt; EndDate )</errorConditionFormula>
        <errorMessage>The start date of the contract must be before the end date of the contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FrameAgreementActivated</fullName>
        <active>true</active>
        <description>This validation rule stops the activation of a contract from the Path component without using the Frame Agreement Button.</description>
        <errorConditionFormula>AND(
  ISCHANGED(Status),
  NOT(ISNEW()),
  ISPICKVAL(Status, &quot;Activated&quot;),
  Grid_isFrameAgreedActivated__c = false
)</errorConditionFormula>
        <errorMessage>Please activate using &apos;Activate Frame Agreement button&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grid_ConditionTermsNotPopulated</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Status ,&quot;Handover&quot;) &amp;&amp; ISPICKVAL(Grid_Condition_Terms__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Grid_Condition_Terms__c</errorDisplayField>
        <errorMessage>Please fill in the Condition Terms</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grid_ContractSendingDateMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Status ,&quot;Contract Sent- Awaiting Client Reply&quot;) &amp;&amp; (ISNULL(Contract_Sending_Date__c) || ISNULL(Offer_Validity__c))</errorConditionFormula>
        <errorMessage>Please fill in the Contract Sending Date and the Offer Validity information</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grid_ContractSigned</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Status ,&quot;Contract Signed&quot;) &amp;&amp; ISNULL(Contract_Signature_Date__c)</errorConditionFormula>
        <errorDisplayField>Contract_Signature_Date__c</errorDisplayField>
        <errorMessage>Please fill in the Contract Signature Date</errorMessage>
    </validationRules>
